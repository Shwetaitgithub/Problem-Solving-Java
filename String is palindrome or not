public  class Main{
public static void main(String[] args) {
    String s="madam";
    int start=0,end=s.length()-1;
    boolean ispalindrome=true;
    while(start<end){
        if(s.charAt(start)!=s.charAt(end)){
            ispalindrome=false;
        }
        start++;
        end--;
    }
    System.out.println(ispalindrome);

}
}
